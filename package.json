{
  "name": "conflated-web-ui-client-service",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "ag-grid-community": "^27.3.0",
    "ag-grid-react": "^27.3.0",
    "apexcharts": "^3.35.3",
    "chart.js": "^3.8.0",
    "chartjs-chart-box-and-violin-plot": "^4.0.0",
    "classnames": "^2.3.1",
    "color": "^4.2.3",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "leaflet": "^1.8.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "noicejs": "^5.0.0-2",
    "oo-redux-utils": "^1.25.0",
    "react": "^18.1.0",
    "react-apexcharts": "^1.4.0",
    "react-compound-slider": "^3.4.0",
    "react-dom": "^18.1.0",
    "react-grid-layout": "^1.3.4",
    "react-leaflet": "^4.0.0",
    "react-notification": "^6.8.5",
    "react-redux": "^8.0.2",
    "react-resizable": "^3.0.4",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-sizeme": "^3.0.2",
    "redux": "^4.2.0",
    "reselect": "^4.1.6",
    "semantic-ui-calendar-react": "^0.15.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.3",
    "semantic-ui-react-numberinput": "^1.5.0",
    "semantic-ui-react-scrollbar": "^1.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env GENERATE_SOURCEMAP=false react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "integration-test": "testcafe chrome integration-tests/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "plugin:css-modules/recommended",
      "plugin:@typescript-eslint/recommended",
      "airbnb-typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "css-modules",
      "@typescript-eslint"
    ],
    "rules": {
      "operator-linebreak": "off",
      "max-len": [
        "error",
        {
          "code": 300
        }
      ],
      "comma-dangle": "off",
      "space-before-function-paren": "off",
      "func-names": "off",
      "indent": "off",
      "function-paren-newline": "off",
      "arrow-parens": "off",
      "max-classes-per-file": [
        "error",
        2
      ],
      "no-use-before-define": "off",
      "no-console": "off",
      "object-curly-newline": "off",
      "class-methods-use-this": "off",
      "no-else-return": "off",
      "implicit-arrow-linebreak": "off",
      "no-confusing-arrow": "off",
      "no-plusplus": "off",
      "css-modules/no-unused-class": "off",
      "import/no-named-default": "off",
      "import/no-cycle": "off",
      "@typescript-eslint/indent": "off",
      "@typescript-eslint/comma-dangle": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "jsx-a11y/label-has-for": "off",
      "react/jsx-curly-newline": "off",
      "react/jsx-props-no-spreading": "off",
      "react/function-component-definition": [2, { "namedComponents": "arrow-function" }],
      "react/require-default-props": [
        1,
        {
          "forbidDefaultForRequired": false
        }
      ],
      "react/default-props-match-prop-types": [
        1,
        {
          "allowRequiredDefaults": true
        }
      ],
      "react/jsx-wrap-multilines": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".tsx"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.17.12",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@cucumber/cucumber": "^8.3.0",
    "@types/color": "^3.0.3",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^28.1.1",
    "@types/leaflet": "^1.7.11",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-grid-layout": "^1.3.2",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-typescript": "^0.14.0",
    "gherkin-testcafe": "^5.4.2",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.2",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.52.3",
    "stylelint": "^14.9.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-at-rule-no-children": "^0.3.1",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-declaration-use-variable": "^1.7.3",
    "stylelint-images": "^2.0.1",
    "stylelint-media-use-custom-media": "^2.0.0",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^4.2.0",
    "stylelint-selector-tag-no-without-class": "^2.0.5",
    "stylelint-use-logical-spec": "^4.1.0",
    "stylelint-use-nesting": "^3.0.0",
    "stylelint-value-no-unknown-custom-properties": "^4.0.0",
    "testcafe": "^1.18.6",
    "typescript": "^4.7.3",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
